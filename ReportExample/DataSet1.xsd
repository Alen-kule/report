                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Invoices.InvoiceNumber, SifCharges.ChargeName, InvoiceDetails_1.Rate,
 CAST(Units as decimal(18,0)) AS Units, 
						 CAST(Units *  Rate as decimal(18,2)) AS Amount, 
						 CAST(Tax as  decimal(16,2)) AS Tax, 
                             (SELECT        CAST(SUM(Units * Rate) + SUM(Tax) AS decimal(18, 2)) AS Expr1
                               FROM            InvoiceDetails
                               WHERE        (InvoiceId = Invoices.Id)) AS t, Clients.Contract, Clients.CompanyName, CAST(Invoices.StartDate AS date) AS StartDate, CAST(Invoices.EndDate AS date) AS EndDate, CAST(Invoices.InvoiceDate AS date) 
                         AS InvoiceDate
FROM            Invoices INNER JOIN
                         InvoiceDetails AS InvoiceDetails_1 ON Invoices.Id = InvoiceDetails_1.InvoiceId INNER JOIN
                         SifCharges ON InvoiceDetails_1.ChargeId = SifCharges.Id INNER JOIN
                         Clients ON Invoices.CompanyName = Clients.CompanyName
WHERE        (Invoices.InvoiceNumber = @invoice)</CommandText>
