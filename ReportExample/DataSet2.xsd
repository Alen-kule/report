                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT       YEAR(Invoices.StartDate) AS 'Year', DATENAME(month,Invoices.StartDate) AS 'Month', SifCharges.ChargeName, 
 CAST((SUM(InvoiceDetails.Units)*SUM(InvoiceDetails.Rate)) as decimal(18,2)) 
                         AS Amount
FROM            Invoices INNER JOIN
                         InvoiceDetails ON Invoices.Id = InvoiceDetails.InvoiceId INNER JOIN
                         SifCharges ON InvoiceDetails.ChargeId = SifCharges.Id INNER JOIN
                         Clients ON Invoices.CompanyName = Clients.CompanyName

						 where       YEAR(Invoices.StartDate)=@year  and    MONTH(Invoices.StartDate)=@month
GROUP BY YEAR(Invoices.InvoiceDate), SifCharges.ChargeName, DATENAME(month,Invoices.StartDate)
,Invoices.StartDate</CommandText>
